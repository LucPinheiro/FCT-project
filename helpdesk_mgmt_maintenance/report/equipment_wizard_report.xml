<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="equipment_report_template">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="web.external_layout">
                    <div class="page">
                        <div class="text-center"><h2>Equipment Report</h2></div>
                        <br/>
                        <br/>                       
                  
                    </div>

                </t>
            </t>
        </t>
    </template>

    <record id="action_report_equipment" model="ir.actions.report">
        <field name="name">Equipment Report</field>
        <field name="model">maintenance.equipment</field>
        <field name="report_type">qweb-html</field>
        <field name="report_name">helpdesk_mgmt_maintenance.equipment_report_template</field>
        <field name="report_file">helpdesk_mgmt_maintenance.equipment_report_template</field>
        <field name="binding_model_id" ref="maintenance.model_maintenance_equipment"/>
        <field name="binding_type">report</field>
    </record>

</odoo>