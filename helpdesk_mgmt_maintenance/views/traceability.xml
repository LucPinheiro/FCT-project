<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="help_main_maintenance_traceability_view_tree" model="ir.ui.view">
        <field name="name">traceability.view.tree (Helpdesk Maintenance)</field>
        <field name="model">tracealibity</field>
        <field name="arch" type="xml">
            <tree string="Traceablity">
                <field name="name" />
                <field name="tracking" widget="radio" />
            </tree>
        </field>
    </record>

    <record id="help_main_maintenance_traceability_view_form" model="ir.ui.view">
        <field name="name">traceability.form (Helpdesk Maintenance)</field>
        <field name="model">tracealibity</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="tracking" widget="radio" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>


</odoo>
